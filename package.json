  "scripts": {
    "dev": "nodemon server",
    "dev:inspect": "nodemon --inspect server",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava",
    "migration": "sequelize db:migrate && sequelize db:seed:all",
