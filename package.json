    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-imports": "^1.5.0",
