    "@nuxtjs/pwa": "^2.0.8",
    "@nuxtjs/sitemap": "0.0.3",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "config": "^1.30.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "minio": "^5.0.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "nuxt": "^1.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.37.4",
    "sequelize-cli": "^4.0.0",
    "toml": "^2.3.3",
    "vee-validate": "^2.0.6",
    "webpack-node-externals": "^1.7.2"
